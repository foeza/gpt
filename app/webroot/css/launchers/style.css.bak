#download-launcher .btn {
  display: inline-block;
  padding: 8px 30px;
  min-width: 100px;
  color: #ffffff;
  font-size: 1.14286em;
  font-weight: 300;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  -webkit-transition: 0.1s ease-in;
  -o-transition: 0.1s ease-in;
  transition: 0.1s ease-in;
  height: auto;
}
#download-launcher .btn.green {
  background-color: #009634;
  -webkit-box-shadow: 0 2px 0 #007d2b;
  box-shadow: 0 2px 0 #007d2b;
}
#download-launcher .btn.green:hover {
  background-color: #008c30;
  -webkit-box-shadow: 0 2px 0 #007228;
  box-shadow: 0 2px 0 #007228;
}
#download-launcher .btn.green:focus, .btn.green:active {
  padding-top: 12px;
  background-color: #007d2b;
  -webkit-box-shadow: inset 0 3px 0 #006322, inset 0 -1px 0 #00a038;
  box-shadow: inset 0 3px 0 #006322, inset 0 -1px 0 #00a038;
}
#download-launcher .btn.border-only {
  border: 1px solid transparent;
  background-color: transparent;
  text-shadow: none;
}
#download-launcher .btn.border-only.white {
  color: #ffffff;
  border-color: #ffffff;
}
#download-launcher .btn.border-only.white:hover {
  color: #1e1e22;
  background-color: #ffffff;
  border-color: transparent;
}
#download-launcher .btn.border-only.white:focus, .btn.border-only.white:active {
  padding-top: 10px;
  padding-bottom: 8px;
  background-color: #f2f2f2;
  border-color: transparent;
  -webkit-box-shadow: inset 0 1px 0 #cccccc;
  box-shadow: inset 0 1px 0 #cccccc;
}

#download-launcher .centered {
  float: none;
  margin: auto;
}

.text-center {
  text-align: center;
}

/* Head */
#download-launcher header {
  width: 100%;
  height: 350px;
  background: #1e1e22 url(../../img/launchers/main-header-bg.jpg) no-repeat bottom center;
  position: relative;
}
#download-launcher header .download-message {
  position: relative;
  height: 350px;
  margin: auto;
  color: #ffffff;
  text-shadow: 0 2px 2px #1e1e22;
}
#download-launcher header .download-message .download-content {
  position: absolute;
  top: 30%;
  left: 15px;
}
#download-launcher header .download-message .download-content a {
  display: inline-block;
  margin-top: 15px;
}
#download-launcher header h1 {
  font-weight: 900;
  font-size: 2em;
  font-family: 'Lato', Helvetica, Arial, sans-serif;
  color: #ffffff;
  text-transform: none;
  line-height: normal;
}
#download-launcher header h2 {
  font-size: 1.28571em;
  font-family: 'Lora', Garamond, Georgia, serif;
  color: #ffffff;
  text-transform: none;
  line-height: normal;
}
#download-launcher header .btn {
  margin-top: 30px;
  font-family: 'Lato', Helvetica, Arial, sans-serif;
}

#download-launcher #content {
  padding: 30px 0;
  background: url(../../img/launchers/p5.png) repeat;
}
#download-launcher #content #unblock {
  margin-top: 90px;
}
#download-launcher #content .title {
  text-align: center;
  font-size: 1.71429em;
  font-weight: 300;
  color: #1e1e22;
  text-transform: none;
}
#download-launcher #content .title:after {
  content: '';
  width: 60px;
  height: 1px;
  background-color: #009634;
  display: block;
  margin: 5px auto;
}
#download-launcher #content [class^="step-"] {
  padding: 100px 0;
  position: relative;
  border-bottom: 1px solid #c8c8c8;
}
#download-launcher #content [class^="step-"]:after {
  content: '';
  display: block;
  clear: both;
}
#download-launcher #content .image-container {
  text-align: center;
}
#download-launcher #content .how-container {
  position: relative;
  padding-top: 60px;
  line-height: 2.14286em;
}
#download-launcher #content .how-container p {
  position: relative;
  font-size: 1.14286em;
  font-family: 'Lora', Garamond, Georgia, serif;
  z-index: 2;
}
#download-launcher #content .how-container .numb {
  position: absolute;
  top: 15px;
  font-size: 7.14286em;
  font-weight: 300;
  font-family: 'Lora', Garamond, Georgia, serif;
  color: rgba(65, 133, 242, 0.8);
  z-index: 1;
}
#download-launcher #content .how-container .numb:after {
  content: '';
  width: 100%;
  height: 30px;
  position: absolute;
  left: 0;
  bottom: -40px;
  background: url(../../img/launchers/p5.png) repeat;
}
#download-launcher #content .step-3 .how-container, #content .step-4 .how-container {
  padding-top: 160px;
}
#download-launcher #content .step-3 .how-container .numb, #content .step-4 .how-container .numb {
  top: 115px;
}
#download-launcher #content .download-foot {
  display: block;
  margin: 60px 0 30px;
  text-align: center;
}
#download-launcher #content #unblock .step-1 .how-container, #content #unblock .step-2 .how-container {
  padding-top: 160px;
}
#download-launcher #content #unblock .step-1 .how-container .numb, #content #unblock .step-2 .how-container .numb {
  top: 115px;
}
#content #unblock .numb:after {
  bottom: -35px;
}
#download-launcher #content #unblock .step-3 {
  margin-bottom: 45px;
}

@media (max-width: 920px) {
  #download-launcher #content .image-container img {
    max-width: 100%;
  }
}
@media (max-width: 767px) {
  #download-launcher header .download-message .download-content {
    top: 0;
    left: 0;
    padding: 60px 30px;
  }

  #download-launcher #content [class^="step-"] {
    padding: 30px 0;
  }
  #download-launcher #content .how-container {
    padding-top: 70px;
  }
  #download-launcher #content .how-container .numb {
    top: 35px;
    font-size: 5.14286em;
  }
  #download-launcher #content .how-container .numb:after {
    bottom: -35px;
  }
  #download-launcher #content .step-3 .how-container, #content .step-4 .how-container {
    padding-top: 70px;
  }
  #download-launcher #content .step-3 .how-container .numb, #content .step-4 .how-container .numb {
    top: 35px;
  }
  #download-launcher #content .step-3 .how-container .numb:after, #content .step-4 .how-container .numb:after {
    bottom: -35px;
  }
}
